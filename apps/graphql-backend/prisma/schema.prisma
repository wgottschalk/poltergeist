generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         Int    @default(autoincrement()) @id
  name       String
  githubName String
  email      String
}

model Team {
  id           Int    @default(autoincrement()) @id
  name         String
  mailingGroup String
}
